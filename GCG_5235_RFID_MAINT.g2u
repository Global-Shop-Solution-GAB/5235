F.Intrinsic.Control.CallSub(ClearOptions)

F.Intrinsic.Control.If(V.DataTable.dtAntennaData.Exists,=,True)
	F.Data.DataTable.Close("dtAntennaData")
F.Intrinsic.Control.EndIf

F.Intrinsic.String.Build("Select * from GCG_5235_RFID_MAINT where antenna = '{0}' and READER = '{1}'",V.Global.sAntenna, V.Global.sReader, V.Local.sSQL)
