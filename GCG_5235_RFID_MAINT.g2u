F.Intrinsic.Control.For(V.Local.iCount,0,V.DataTable.dtAntennaData.RowCount--,1)
	V.Local.sTransCode.Set(V.DataTable.dtAntennaData(V.Local.iCount).TX_CODE!fieldVal)
	F.Intrinsic.Control.SelectCase(V.Local.sTransCode)
		F.Intrinsic.Control.Case("J55")
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).Enabled!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkJ55.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtWCJ55.Text(V.DataTable.dtAntennaData(V.Local.iCount).WC!FieldVal)
