Program.Sub.CheckAntennaList.Start
F.Intrinsic.Control.Try

F.Data.DataTable.CreateFromSQL("AntList","Con","SELECT distinct Antenna, Reader from GCG_5235_RFID_HDWR where CONCAT(antenna, Reader) not in (Select distinct CONCAT(antenna, Reader) from GCG_5235_RFID_MAINT WHERE TX_CODE = 'J55') ")
F.Intrinsic.Control.If(V.DataTable.AntList.RowCount,<>,0)
	F.Data.DataTable.AddColumn("AntList","TX_CODE","String")
	F.Data.DataTable.SetValue("AntList",-1,"TX_CODE","J55")
	F.Data.DataTable.AddColumn("AntList","ENABLED","BOOLEAN")
	F.Data.DataTable.SetValue("AntList",-1,"ENABLED",False)
	F.Data.DataTable.AddColumn("AntList","PRINT_FLAG","BOOLEAN")
	F.Data.DataTable.SetValue("AntList",-1,"PRINT_FLAG",False)
	F.Data.DataTable.AddColumn("AntList","NOTIFY_FLAG","BOOLEAN")
	F.Data.DataTable.SetValue("AntList",-1,"NOTIFY_FLAG",False)
	F.Data.DataTable.SaveToDB("AntList","Con","GCG_5235_RFID_MAINT","ANTENNA*!*READER*!*TX_CODE",256,"ANTENNA@!@ANTENNA*!*TX_CODE@!@TX_CODE*!*ENABLED@!@ENABLED*!*PRINT_FLAG@!@PRINT_FLAG*!*NOTIFY_FLAG@!@NOTIFY_FLAG*!*READER@!@READER")
F.Intrinsic.Control.EndIf
F.Data.DataTable.Close("AntList")

