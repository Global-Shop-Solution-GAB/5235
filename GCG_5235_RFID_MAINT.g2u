				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkPrintJ55.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtPrinterJ55.Text(V.DataTable.dtAntennaData(V.Local.iCount).PRINTER!FieldVal)
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).NOTIFY_FLAG!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkNotifyJ55.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtNotifyJ55.Text(V.DataTable.dtAntennaData(V.Local.iCount).REC_EMAILID!FieldVal)
			GUI.FAntennaMaint.txtSenderJ55.Text(V.DataTable.dtAntennaData(V.Local.iCount).SENDER_EMAILID!FieldVal)
			F.intrinsic.Control.CallSub(ToggleTransactionEnabled, "CONTROLNAME", "chkJ55")
			F.Intrinsic.Control.CallSub(CheckPrinterClicked, "CONTROLNAME", "chkPrintJ55")
			F.intrinsic.Control.CallSub(chkNotifyJ55_Click)
			
			'load extra option
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim, !=, "")
				GUI.FAntennaMaint.chkJ55OnlyIssueFromDashboard.Value(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim)
			F.Intrinsic.Control.EndIf
			
		F.Intrinsic.Control.Case("J52")
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).Enabled!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkJ52.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtToBinJ52.Text(V.DataTable.dtAntennaData(V.Local.iCount).TOBIN!FieldVal)
			
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).PRINT_FLAG!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkPrintJ52.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtPrinterJ52.Text(V.DataTable.dtAntennaData(V.Local.iCount).PRINTER!FieldVal)
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).NOTIFY_FLAG!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkNotifyJ52.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtNotifyJ52.Text(V.DataTable.dtAntennaData(V.Local.iCount).REC_EMAILID!FieldVal)
			GUI.FAntennaMaint.txtSenderJ52.Text(V.DataTable.dtAntennaData(V.Local.iCount).SENDER_EMAILID!FieldVal)
			
			F.Intrinsic.String.Split(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim, "!@#$#@!", V.Local.sExtraOption)
			F.Intrinsic.Control.If(V.Local.sExtraOption(0), =, "DEFAULT")
				GUI.FAntennaMaint.chkDefaultBin.Value(True)
			F.Intrinsic.Control.ElseIf(V.Local.sExtraOption(0), =, "STOCKING")
				GUI.FAntennaMaint.chkStockingBin.Value(True)
			F.Intrinsic.Control.EndIf
			
			F.Intrinsic.Control.If(V.Local.sExtraOption.UBound, >, 0)
				GUI.FAntennaMaint.chkOverrideLabelQuantity.Value(V.Local.sExtraOption(1))
			F.Intrinsic.Control.EndIf
			
			F.Intrinsic.Control.If(V.Local.sExtraOption.UBound, >, 1)
				GUI.FAntennaMaint.ddlCloseJ52.Text(V.Local.sExtraOption(2).Trim)
			F.Intrinsic.Control.EndIf
			
			F.intrinsic.Control.CallSub(ToggleTransactionEnabled, "CONTROLNAME", "chkJ52")
			F.Intrinsic.Control.CallSub(CheckPrinterClicked, "CONTROLNAME", "chkPrintJ52")
			F.intrinsic.Control.CallSub(chkNotifyJ52_Click)
		
		F.Intrinsic.Control.Case("O90")
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).Enabled!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkO90.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtToBinO90.Text(V.DataTable.dtAntennaData(V.Local.iCount).TOBIN!FieldVal)
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).PRINT_FLAG!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkPrintO90.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtPrinterO90.Text(V.DataTable.dtAntennaData(V.Local.iCount).PRINTER!FieldVal)
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).NOTIFY_FLAG!FieldVal,=,"True")
				V.Local.iflag.Set(1)
			F.Intrinsic.Control.Else
				V.Local.iflag.Set(0)
			F.Intrinsic.Control.EndIf
			GUI.FAntennaMaint.chkNotifyO90.Value(V.Local.iflag)
			GUI.FAntennaMaint.txtNotifyO90.Text(V.DataTable.dtAntennaData(V.Local.iCount).REC_EMAILID!FieldVal)
			GUI.FAntennaMaint.txtSenderO90.Text(V.DataTable.dtAntennaData(V.Local.iCount).SENDER_EMAILID!FieldVal)
			F.intrinsic.Control.CallSub(ToggleTransactionEnabled, "CONTROLNAME", "chkO90")
			F.Intrinsic.Control.CallSub(CheckPrinterClicked, "CONTROLNAME", "chkPrintO90")
			F.intrinsic.Control.CallSub(chkNotifyO90_Click)
			
		F.Intrinsic.Control.Case("L01")
			GUI.FAntennaMaint.chkL01.Value(V.DataTable.dtAntennaData(V.Local.iCount).ENABLED!FieldVal)
			GUI.FAntennaMaint.txtL01Workcenter.Text(V.DataTable.dtAntennaData(V.Local.iCount).WC!FieldValTrim)
			GUI.FAntennaMaint.chkNotifyL01.Value(V.DataTable.dtAntennaData(V.Local.iCount).NOTIFY_FLAG!FieldVal)
			GUI.FAntennaMaint.txtNotifyL01.Text(V.DataTable.dtAntennaData(V.Local.iCount).REC_EMAILID!FieldValTrim)
			GUI.FAntennaMaint.txtSenderL01.Text(V.DataTable.dtAntennaData(V.Local.iCount).SENDER_EMAILID!FieldValTrim)
			
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim, !=, "")
				F.Intrinsic.String.Split(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim, "!@#$#@!", V.Local.sExtraOption)
				F.Intrinsic.Control.If(V.Local.sExtraOption.UBound, >=, 2)
					GUI.FAntennaMaint.txtL01Employee.Text(V.Local.sExtraOption(0).Trim)
					GUI.FAntennaMaint.ddlClose.Text(V.Local.sExtraOption(1).Trim)
					GUI.FAntennaMaint.chkPORequest.Value(V.Local.sExtraOption(2))
				F.Intrinsic.Control.EndIf
				
			F.Intrinsic.Control.EndIf
			
			F.intrinsic.Control.CallSub(ToggleTransactionEnabled, "CONTROLNAME", "chkL01")
			F.intrinsic.Control.CallSub(chkNotifyL01_Click)
			
		F.Intrinsic.Control.Case("P10")
			GUI.FAntennaMaint.chkP10.Value(V.DataTable.dtAntennaData(V.Local.iCount).ENABLED!FieldVal)
			GUI.FAntennaMaint.chkNotifyP10.Value(V.DataTable.dtAntennaData(V.Local.iCount).NOTIFY_FLAG!FieldVal)
			GUI.FAntennaMaint.txtNotifyP10.Text(V.DataTable.dtAntennaData(V.Local.iCount).REC_EMAILID!FieldValTrim)
			GUI.FAntennaMaint.txtSenderP10.Text(V.DataTable.dtAntennaData(V.Local.iCount).SENDER_EMAILID!FieldValTrim)
			F.intrinsic.Control.CallSub(ToggleTransactionEnabled, "CONTROLNAME", "chkP10")
			F.intrinsic.Control.CallSub(chkNotifyP10_Click)
			
			F.Intrinsic.Control.If(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim, =, "INSPECTION")
				GUI.FAntennaMaint.optP10Inspection.Value(1)
			F.Intrinsic.Control.ElseIf(V.DataTable.dtAntennaData(V.Local.iCount).EXTRA_OPTION!FieldValTrim, =, "JOB")
				GUI.FAntennaMaint.optP10Job.Value(1)
			F.Intrinsic.Control.Else
				GUI.FAntennaMaint.optP10Default.Value(1)
			F.Intrinsic.Control.EndIf
			
		F.Intrinsic.Control.Case("S99")
			GUI.FAntennaMaint.chkS99.Value(V.DataTable.dtAntennaData(V.Local.iCount).ENABLED!FieldVal)
			F.intrinsic.Control.CallSub(ToggleTransactionEnabled, "CONTROLNAME", "chkS99")
			
			GUI.FAntennaMaint.txtAPIUrl.Text(V.DataTable.dtAntennaData(V.Local.iCount).API_URL!FieldValTrim)
			GUI.FAntennaMaint.txtReaderDeviceID.Text(V.DataTable.dtAntennaData(V.Local.iCount).DEVICE_ID!FieldValTrim)
			GUI.FAntennaMaint.txtRedPortMap.Text(V.DataTable.dtAntennaData(V.Local.iCount).RED_PORT!FieldValTrim)
			GUI.FAntennaMaint.txtYellowPortMap.Text(V.DataTable.dtAntennaData(V.Local.iCount).YELLOW_PORT!FieldValTrim)
